

#include <iostream>
using namespace std;
void headr() {
    cout <<"\t \t \tNumbers * Numbers \t \t" << endl;
    for (int i = 1; i <= 10; i++) {
        cout << " "<<i<<"\t";
        
    }
    cout << "\n__________________________________________________________________________\n";
}
string tap(int ii) {
    if (ii < 10) {
        return "  |";
    }
    else return " |";
}
void index() {
    headr();

    for (int i = 1; i <= 10; i++) {

        cout <<i<< tap(i) << "\t ";
        for (int k = 1; k <= 10; k++)
        {
            cout << i * k << "\t";

        }
        cout << endl;
    }
}
int main()
{
    index();
    return 0;
}
