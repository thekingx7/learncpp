

#include <iostream>
#include <cmath>

using namespace std;
float swap(float num, float num2) {

    return num*sqrt(num2*num2-num*num);
}

int main()
{
    float x;
    float y;

    cout << "num1 : " << endl;
    cin >> x;
    cout << "num 2 : " << endl;
    cin >> y;
    cout << swap(x, y);



    return 0;
}

