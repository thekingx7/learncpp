

#include <iostream>
using namespace std;
void Procedures() {
    int num1;
    int num2;
    cout << "enter num1 : " << endl;
    cin >> num1;
    cout << "enter num2 : " << endl;
    cin >> num2;
    cout << num1 + num2;

}
int fun() {
    int num1;
    int num2;
    cout << "enter num1 : " << endl;
    cin >> num1;
    cout << "enter num2 : " << endl;
    cin >> num2;
   return  num1 + num2;

}

int main()
{
    Procedures();
    cout << fun() << endl;


    return 0;
}

