

#include <iostream>
using namespace std;
void Procedures() {
    int num1;
    int num2;
    cout << "enter num1 : " << endl;
    cin >> num1;
    cout << "enter num2 : " << endl;
    cin >> num2;
    cout << num1 + num2;

}
int fun() {
    int num1;
    int num2;
    cout << "enter num1 : " << endl;
    cin >> num1;
    cout << "enter num2 : " << endl;
    cin >> num2;
   return  num1 + num2;

}

int main()
{
    Procedures();
    cout << fun() << endl;


    return 0;
}

////////////////////////////////////


#include <iostream>
using namespace std;
int swap(int num, int num2) {
    int tem = num;//10
    num = num2;//20
    num2 = tem;
    return num,num2;
}

int main()
{
    int x;
    int y;

    cout << "num1" << endl;
    cin >> x;
    cout << "num 2 : " << endl;
    cin >> y;
    cout << "one "<<x << endl <<"tow "<< y << endl;
    int tem = x;//10
    x = y;//20
    y = tem;
    cout << "one " << x << endl << "tow " << y << endl;



    return 0;
}

